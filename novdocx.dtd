<!-- lists -->
<!ELEMENT itemizedlist  (title?, remark*, listitem+) >
<!ELEMENT orderedlist   (title?, remark*, listitem+) >
<!ELEMENT simplelist    (member+) >
<!ELEMENT variablelist  (title?, remark*, varlistentry+) >
